{
  "name": "start",
  "version": "0.0.1",
  "description": "neatly packaged best practices and opinions",
  "main": "main.js",
  "scripts": {
    "go": "yarn build && yarn start",
    "build": "parcel build src/client/main.tsx --out-dir build --out-file app.js",
    "makedb": "dropdb start && createdb start && psql -1 --dbname=start --file='./src/server/Schema.sql'",
    "test": "jest",
    "start": "babel-node --extensions '.ts,.tsx' src/server/main.tsx"
  },
  "repository": {
    "type": "git",
    "url": "."
  },
  "keywords": [
    "template"
  ],
  "dependencies": {
    "babel-core": "*",
    "babel-preset-env": "*",
    "babel-preset-react": "*",
    "babel-preset-stage-0": "*",
    "babel-preset-stage-1": "*",
    "babel-preset-stage-2": "*",
    "babel-preset-stage-3": "*",
    "body-parser": "*",
    "cookie-parser": "*",
    "dotenv": "*",
    "express": "*",
    "express-session": "*",
    "parcel-bundler": "*",
    "passport": "*",
    "passport-github": "*",
    "passport-twitter": "*",
    "pg": "*",
    "react": "*",
    "react-dom": "*",
    "ws": "*"
  },
  "optionalDependencies": {
    "pg-native": "*"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions",
              "safari >= 10"
            ]
          },
          "exclude": [
            "transform-regenerator",
            "transform-es2015-classes",
            "transform-es2015-arrow-functions"
          ]
        }
      ],
      [
        "react"
      ],
      [
        "stage-0"
      ],
      [
        "stage-1"
      ],
      [
        "stage-2"
      ],
      [
        "stage-3"
      ]
    ]
  },
  "author": "xhjkl",
  "license": "UNLICENSED",
  "private": true,
  "devDependencies": {
    "@types/express": "^4.11.1",
    "@types/jest": "^22.2.2",
    "@types/node": "^10.0.0",
    "@types/react": "^16.0.41",
    "tslint": "^5.9.1",
    "tslint-config-standard": "^7.0.0",
    "typescript": "^2.7.2"
  }
}
